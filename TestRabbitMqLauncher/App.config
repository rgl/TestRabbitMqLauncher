<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- serilog:minimum-level can be one of: Verbose, Debug, Information, Warning, Error, Fatal -->
    <add key="serilog:minimum-level" value="Debug" />
    <add key="serilog:using:File" value="Serilog.Sinks.File" />
    <add key="serilog:write-to:File.path" value="logs\TestRabbitMqLauncher-.log" />
    <add key="serilog:write-to:File.rollingInterval" value="Day" />
    <add key="serilog:write-to:File.retainedFileCountLimit" value="31" />
  </appSettings>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>